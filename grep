touch "file.txt"
echo "This is some new content to add......" >> "file.txt"
echo "my name is Monu Tiwari.."
echo "grep in process.."
grep "Monu" "file.txt"
echo "sed is in process to replace old_string"
sed 's/old_string/new_string/g' "file.txt"
echo "awak is in proces to print line 2 and 3"
awk '{print $2, $1}' "file.txt"
